{
    "name": "nickfairchild/cf7-turnstile",
    "description": "WordPress plugin to add Cloudflare's Turnstile recaptcha to Contact Form 7.",
    "type": "wordpress-plugin",
    "keywords": [
        "wordpress",
        "contact form 7",
        "cloudflare",
        "turnstile",
        "recaptcha"
    ],
    "homepage": "https://github.com/nickfairchild/cf7-turnstile",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Nick Fairchild",
            "email": "nick.fairchild@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "composer/installers": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Nickfairchild\\CF7Turnstile\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
